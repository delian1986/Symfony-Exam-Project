{% extends 'base.html.twig' %}

{% block main %}
    <div class="row">
        <div class="col-sm-6">
            {{ form_start(form) }}
            <div class="form-group">
                {{ form_label(form.initialCash, 'Add Cash: ') }}
                {{ form_widget(form.initialCash, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="form-group">
                {{ form_widget(form.Save, {'attr': {'class': 'btn btn-primary'}}) }}
            </div>
        </div>
        {{ form_end(form) }}
    </div>

{% endblock %}

